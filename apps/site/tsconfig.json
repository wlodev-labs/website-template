{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["./src/components/*"],
            "@blocks/*": ["./src/blocks/*"],
            "@utils/*": ["./src/utils/*"],
            "@layouts/*": ["./src/layouts/*"],
            "@config/*": ["./src/config/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}
